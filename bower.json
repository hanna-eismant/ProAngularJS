    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.2",
    "jquery-ui": "~1.11.4",
    "angular": "~1.3.14",
    "angular-route": "~1.3.14",
    "angular-resource": "~1.3.14",
    "angular-i18n": "~1.3.15"
